   "outputs": [],
   "source": [
    "# data_train_test.loc[[1017688,1019400],'Open']=1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [],
   "source": [
    "data_train_test.Open.fillna(1,inplace=True)\n"
   ]
